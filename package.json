  "description": "A Probot app",
  "author": "Weiwen Zhao <Weiwen.Zhao@ibm.com>",
  "license": "ISC",
  "repository": "https://github.com//ticket-manager.git",
  "homepage": "https://github.com//ticket-manager",
  "bugs": "https://github.com//ticket-manager/issues",
